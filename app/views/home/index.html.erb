<%= render :partial => "latest_beats", :object => @latest_tracks, :as => 'tracks' %>

<%= render :partial => "popular_tutorials", :object => @tutorials, :as => 'tutorials' %>

<%= render :partial => "recent_discussions", :object => @discussions, :as => 'discussions' %>
<script type="text/javascript">
$(function() {
  Sensori.HomeView = new Sensori.Views.Home({
    discussions: new Sensori.Collections.Discussions(<%= raw @discussions.to_json %>),
    el: $('.home-container')
  });
});
</script>
